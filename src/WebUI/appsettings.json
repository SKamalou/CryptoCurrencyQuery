{
  "UseInMemoryDatabase": true,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=CryptoCurrencyQueryDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "WaitAndRetryConfig": {
    "Wait": 500,
    "Retry": 2,
    "Timeout": 2000
  },
  "CryptoCurrencyApiConfig": {
    "BaseAddress": "https://pro-api.coinmarketcap.com",
    "ApiKeyHeaderKey": "X-CMC_PRO_API_KEY",
    "ApiKey": "YourApiKey"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CachingConfig": {
    "CryptoCurrenciesExpireTimeInMinute": 120
  },
  "AllowedHosts": "*"
}
